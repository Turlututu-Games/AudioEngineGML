{
  "configs": [
    {
      "useRecommended": true,
      "rules": {
        "missing-enum-value": "off",
        "missing-event-description": "off",
        "function-comment-params-description": "warning",
        "function-comment-returns": "warning",
        "function-comment-tags": [
          "warning",
          {
            "examplePublicOnly": false,
            "fixUrlBase": "https://turlututu-games.github.io/AudioEngineGML"
          }
        ],
        "function-comment-returns-description": "warning",
        "function-name": [
          "warning",
          {
            "privateClass": "^__(AE|AudioEngine)[A-Z][A-Za-z]",
            "privateFunction": "^__(AE|AudioEngine)[A-Z][A-Za-z]",
            "publicClass": "^AudioEngine[A-Z]",
            "publicFunction": "^AudioEngine[A-Z]"
          }
        ]
      }
    },
    {
      "ignores": [
        "scripts/AudioEngineDefineEnums/AudioEngineDefineEnums.gml"
      ],
      "rules": {
        "missing-enum-value": "warning"
      }
    },
    {
      "ignores": [
        "objects/oExample",
        "objects/oEmitter"
      ],
      "rules": {
        "missing-event-description": [
          "warning",
          {
            "onlyAlarmsAndEvents": false
          }
        ]
      }
    }
  ]
}